{
   "containerDefinitions": [
      {

         "essential": true,
         "image":  "public.ecr.aws/i4s9m7d8/orca:${{ github.sha }}",
         "name": "orca-exercise",
         "portMappings": [
            {
               "containerPort": 5000,
               "hostPort": 5000,
               "protocol": "tcp"
            }
         ]
      }
   ],
   "cpu": "256",
   "executionRoleArn": "arn:aws:iam::012345678910:role/ecsTaskExecutionRole",
   "family": "fargate-task-definition",
   "memory": "512",
   "networkMode": "awsvpc",
   "runtimePlatform": {
        "operatingSystemFamily": "LINUX"
    },
   "requiresCompatibilities": [
       "FARGATE"
    ]
}