{
    "family": "orca-fargate",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "secrets": [{
                "name": "DATABASE_URL",
                "valueFrom": "arn:aws:secretsmanager:eu-west-1.:022786159174:secret:DATABASE_URL"
            }],
            "name": "orca-exercise-app",
            "image": "orca-exercise",
            "portMappings": [
                {
                    "containerPort": 5000,
                    "hostPort": 5000,
                    "protocol": "tcp"
                    }
            ],
            "essential": true,
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
}