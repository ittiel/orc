{
   "containerDefinitions": [
      {
         "essential": true,
         "image":  "022786159174.dkr.ecr.eu-west-1.amazonaws.com/orca:${{ github.sha }}",
         "name": "orca-exercise",
         "portMappings": [
            {
               "containerPort": 5000,
               "hostPort": 5000,
               "protocol": "tcp"
            }
         ]
      }
   ],
   "cpu": "256",
   "family": "fargate-task-definition",
   "memory": "512",
   "networkMode": "awsvpc",
   "runtimePlatform": {
        "operatingSystemFamily": "LINUX"
    },
   "requiresCompatibilities": [
       "FARGATE"
    ]
}